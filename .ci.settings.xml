<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
	<servers>
		<server>
			<id>github</id>
			<username>${env.github_username}</username>
			<password>${env.github_token}</password>
		</server>
		<server>
			<id>sonatype-nexus-staging</id>
			<username>${env.sonatype_username}</username>
			<password>${env.sonatype_password}</password>
		</server>
		<server>
			<id>sonatype-nexus-snapshots</id>
			<username>${env.sonatype_username}</username>
			<password>${env.sonatype_password}</password>
		</server>
	</servers>

	<mirrors>
		<mirror>
			<id>bits-netbeans-http-blocker</id>
			<mirrorOf>bits-netbeans</mirrorOf>
			<name>Bits NetBeans Mirror</name>
			<url>http://netbeans.apidesign.org/maven2/</url>
		</mirror>
		<mirror>
			<id>scijava-http-blocker</id>
			<mirrorOf>scijava</mirrorOf>
			<name>SciJava Mirror</name>
			<url>https://maven.scijava.org/content/groups/public/</url>
		</mirror>
		<mirror>
			<id>central-http-blocker</id>
			<mirrorOf>central</mirrorOf>
			<name>Maven Central Mirror</name>
			<url>https://repo1.maven.org/maven2/</url>
		</mirror>
		<mirror>
			<id>apache-http-blocker</id>
			<mirrorOf>apache</mirrorOf>
			<name>Apache Mirror</name>
			<url>https://repo.maven.apache.org/maven2/</url>
		</mirror>
		<mirror>
			<id>sonatype-nexus-staging-http-blocker</id>
			<mirrorOf>sonatype-nexus-staging</mirrorOf>
			<name>Sonatype Nexus Staging Mirror</name>
			<url>https://oss.sonatype.org/service/local/staging/deploy/maven2/</url>
		</mirror>
		<mirror>
			<id>sonatype-nexus-snapshots-http-blocker</id>
			<mirrorOf>sonatype-nexus-snapshots</mirrorOf>
			<name>Sonatype Nexus Snapshots Mirror</name>
			<url>https://oss.sonatype.org/content/repositories/snapshots</url>
		</mirror>
	</mirrors>

	<activeProfiles>
		<activeProfile>ci</activeProfile>
	</activeProfiles>

	<profiles>
		<profile>
			<id>ci</id>
			<properties>
				<repoToken>${env.repoToken}</repoToken>
				<gpg.passphrase>${env.signing_keypass}</gpg.passphrase>
				<skipRemoteStaging>${env.skipRemoteStaging}</skipRemoteStaging>
			</properties>
			<repositories>
				<repository>
					<id>bits-netbeans</id>
					<name>bits-netbeans</name>
					<url>http://netbeans.apidesign.org/maven2/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>scijava</id>
					<name>scijava</name>
					<url>https://maven.scijava.org/content/groups/public/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>central</id>
					<name>central</name>
					<url>https://repo1.maven.org/maven2/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>apache</id>
					<name>apache</name>
					<url>https://repo.maven.apache.org/maven2/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>bits-netbeans</id>
					<name>bits-netbeans</name>
					<url>http://netbeans.apidesign.org/maven2/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>scijava</id>
					<name>scijava</name>
					<url>https://maven.scijava.org/content/groups/public/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>central</id>
					<name>central</name>
					<url>https://repo1.maven.org/maven2/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>apache</id>
					<name>apache</name>
					<url>https://repo.maven.apache.org/maven2/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
</settings>
